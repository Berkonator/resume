<div>
    {{#with main}}
    <main>
        <section>
            {{> title data="My educationss" big=true}}
            <ul>
                {{#each education}}
                <li>
                    {{name}}
                    {{#unless isEnd}} (навчання триває) {{/unless}}
                </li>
                {{/each}}
            </ul>
            {{> title data="My certificates"}}
            <ul>
                {{#each certificates}}
                <li>
                    {{name}}
                    ({{id}})
                </li>
                {{/each}}
            </ul>
        </section>
    </main>
    {{/with}}
</div>