<h1>{{name}}</h1>
<br><br>
<h3>{{description}}</h3>
<br>
{{#with history}}
<div><b>Founder:</b> {{founder}}, <em>{{year}}</em></div><br><hr>
{{/with}}
<h3>Types:</h3>
<ul>
    {{#each types}}
    <li>
        {{this}}
    </li>
    {{/each}}
</ul><hr>
<h3>Syntax:</h3>
<div>
    {{#with syntax}}
            <ol>
            <li>Variables:</li><br>
            <ul>
                {{#each variables}}
                <li>
                    {{this}}
                </li>
                {{/each}}
            </ul>
            <br><li>Functions:</li><br>
            <ul>
                {{#each functions}}
                <li>
                    {{this}}
                </li>
                {{/each}}
            </ul>
            <br><li>Conditionals:</li><br>
            <ul>
                {{#each conditionals}}
                <li>
                    {{this}}
                </li>
                {{/each}}
            </ul>
            <br><li>Loops:</li><br>
            <ul>
                {{#each loops}}
                <li>
                    {{this}}
                </li>
                {{/each}}
            </ul>
            <br><li>Classes:</li><br>
            <ul>
                {{#each classes}}
                <li>
                    {{this}}
                </li>
                {{/each}}
            </ul>
            </ol>   
    {{/with}}
</div><hr>
<h3>Features:</h3>
<ul>
    {{#each features}}
    <li>{{this}}</li>
    {{/each}}
</ul><hr>
<h3>Libraries:</h3>
<ul>
    {{#each libraries}}
    <li>{{this}}</li>
    {{/each}}
</ul><hr>
<h3>Tools:</h3>
<ul>
    {{#each tools}}
    <li>{{this}}</li>
    {{/each}}
</ul><hr>
<h3>Community</h3>
<ul>
    {{#each community}}
    <li>
        {{name}} /
        {{type}}
    </li>
    {{/each}}
</ul>